    "imap-simple": "^5.1.0",
    "mailparser": "^3.9.0",
    "mammoth": "^1.11.0",
    "minio": "^8.0.6",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.10",
    "openai": "^6.9.1",
    "pdf-parse": "^2.3.0",
    "pdfkit": "^0.17.2",
    "prisma": "^6.17.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
